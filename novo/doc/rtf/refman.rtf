241         self.curs.execute("SELECT funcionarios.nome, funcionarios.id_funcionario, horarios.hora_inicial, horarios.hora_final FROM horarios INNER JOIN funcionarios ON horarios.id_funcionario=funcionarios.id_funcionario WHERE horarios.dia_da_semana=%s AND curtime() >= subtime(horarios.hora_inicial,%s) AND curtime() <= addtime(horarios.hora_final,%s)",(dia_da_semana,limite_inferior,limite_superior))\par
242         linhas = self.curs.fetchall()\par
243         l=\{\}\par
244         if len(linhas)>0:\par
245             for i in linhas:\par
246                 l[str(i[1])]=\{'nome':i[0],'hora_inicial':i[2],'hora_final':i[3]\}\par
247         self.conn.commit()\par
248         return l if len(l)>0 else False\par
\par
}
}
