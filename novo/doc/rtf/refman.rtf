135             return False\par
136         i=0\par
137         for i in range(len(self.funcionarios_horario_list)):\par
138             if isinstance(id_funcionario, str) or isinstance(id_funcionario, unicode):\par
139                 if self.funcionarios_horario_list[i]['id_funcionario']==id_funcionario:\par
140                     self.model.takeRow(i)\par
141                     del self.funcionarios_horario_list[i]\par
142                     break\par
143             elif isinstance(func, str) or isinstance(func, unicode):\par
144                 if self.funcionarios_horario_list[i]['func']==func:\par
145                     self.model.takeRow(i)\par
146                     del self.funcionarios_horario_list[i]\par
147                     break\par
148         return True\par
\par
}
