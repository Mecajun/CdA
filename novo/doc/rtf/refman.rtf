143     def verifica_Ja_Existe(self,nome=None,matricula=None,rfid=None,id_funcionario=None):\par
144         if nome:\par
145             sql="SELECT count(*) FROM funcionarios WHERE nome=%s AND ativo=true"\par
146             tup=(nome)\par
147             if id_funcionario!=None:\par
148                 sql=sql+" AND id_funcionario!=%s"\par
149                 tup=(nome,id_funcionario)\par
150             self.curs.execute(sql,tup)\par
151             nome = self.curs.fetchall()\par
152             nome = nome[0][0]\par
153             if nome==0:\par
154                 nome = None\par
155             else: \par
156                 nome = True\par
157         if matricula:\par
158             sql="SELECT count(*) FROM funcionarios WHERE matricula=%s AND ativo=true"\par
159             tup=(matricula)\par
160             if id_funcionario!=None:\par
161                 sql=sql+" AND id_funcionario!=%s"\par
162                 tup=(matricula,id_funcionario)\par
163             self.curs.execute(sql,tup)\par
164             matricula = self.curs.fetchall()\par
165             matricula = matricula[0][0]\par
166             if matricula==0: \par
167                 matricula = None\par
168             else:\par
169                 matricula = True\par
170         if rfid:\par
171             sql="SELECT count(*) FROM funcionarios WHERE rfid=%s AND ativo=true"\par
172             tup=(rfid)\par
173             if id_funcionario!=None:\par
174                 sql=sql+" AND id_funcionario!=%s"\par
175                 tup=(rfid,id_funcionario)\par
176             self.curs.execute(sql,tup)\par
177             rfid = self.curs.fetchall()\par
178             rfid = rfid[0][0]\par
179             if rfid==0: \par
180                 rfid = None\par
181             else:\par
182                 rfid = True\par
183 \par
184         temp=nome or matricula or rfid;\par
185         if temp==None:\par
186             temp=False\par
187         self.conn.commit()\par
188         return \{'nome':nome,'matricula':matricula,'rfid':rfid,'existe':temp\}\par
\par
