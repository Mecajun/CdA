166                         item.setIcon(QPixmap("imagens/yes_icon.png"))\par
167                     else:\par
168                         item.setIcon(QPixmap("imagens/no_icon.png"))\par
169                     self.model.insertRow(i,item)\par
170                     self.funcionarios_horario_list[i]['estado']=estado\par
171                     return True\par
172             elif isinstance(func, str) or isinstance(func, unicode):\par
173                 if self.funcionarios_horario_list[i]['func']==func:\par
174                     if self.funcionarios_horario_list[i]['estado']==estado:\par
175                         return True\par
176                     self.model.takeRow(i)\par
177                     item = QStandardItem(self.funcionarios_horario_list[i]['func'])\par
178                     if estado==True:\par
179                         item.setIcon(QPixmap("imagens/yes_icon.png"))\par
180                     else:\par
181                         item.setIcon(QPixmap("imagens/no_icon.png"))\par
182                     self.model.insertRow(i,item)\par
183                     self.funcionarios_horario_list[i]['estado']=estado\par
184                     return True\par
185         return True\par
\par
