77     def atualiza_Funcionarios_Esperados(self):\par
78         tol_ent_ant=self.db.obter_Configuracoes("tol_ent_ant")\par
79         tol_sai_dep=self.db.obter_Configuracoes("tol_sai_dep")\par
80         esperados=self.db.buscar_Funcionarios_Esperados(get_Week_Day(),tol_ent_ant,tol_sai_dep)\par
81         funcionarios_horario_list_ids=[x['id_funcionario'] for x in self.funcionarios_horario_list]\par
82 \par
83         if esperados==False and len(funcionarios_horario_list_ids)>0:\par
84             for i in funcionarios_horario_list_ids:\par
85                 self.remove_Funcionarios_Horario(None,i)\par
86 \par
87         if esperados==False and len(funcionarios_horario_list_ids)==0:\par
88             return True\par
89 \par
90         self.logados=self.db.buscar_Funcionarios_Esperados_Logados(get_Week_Day(),tol_ent_ant,tol_sai_dep)\par
91         self.nao_logados=[]\par
92         if self.logados==False:\par
93             self.nao_logados=[x for x in esperados.keys()]\par
94             self.logados=[]\par
95         else:\par
96             self.nao_logados=[x for x in esperados.keys() if x not in self.logados]\par
97 \par
98         for i in esperados.keys():\par
99             if i not in funcionarios_horario_list_ids:\par
100                 if i in self.logados:\par
101                     self.adiciona_Funcionarios_Horario(esperados[i]['nome'],i,True)\par
102                 if i in self.nao_logados:\par
103                     self.adiciona_Funcionarios_Horario(esperados[i]['nome'],i,False)\par
104             else:\par
105                 if i in self.logados:\par
106                     self.atualiza_Funcionarios_Horario(None,i,True)\par
107                 if i in self.nao_logados:\par
108                     self.atualiza_Funcionarios_Horario(None,i,False) \par
109 \par
110         for i in funcionarios_horario_list_ids:\par
111             if i not in esperados.keys():\par
112                 self.remove_Funcionarios_Horario(None,i)\par
\par
}
