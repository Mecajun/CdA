308     def dar_Ponto(self,id_funcionario):\par
309         # Obtem os limites de tempo para considerar o ponto entrada\par
310         limite_inferior_entrada=string_2_Timedelta(self.db.obter_Configuracoes('tol_ent_ant'))\par
311         limite_superior_entrada=string_2_Timedelta(self.db.obter_Configuracoes('tol_ent_dep'))\par
312         #  Obtem os limites de tempo para considerar o ponto de saida\par
313         limite_inferior_saida=string_2_Timedelta(self.db.obter_Configuracoes('tol_sai_ant'))\par
314         limite_superior_saida=string_2_Timedelta(self.db.obter_Configuracoes('tol_sai_dep'))\par
315 \par
316         ponto_antigo=self.db.buscar_Ponto_Aberto_de_Funcionario(id_funcionario)\par
317 \par
318         if ponto_antigo!=False:\par
319             #   Ponto que n\'E3o foi fechado\par
320             if (ponto_antigo['hora_final']+limite_superior_saida) < data_Atual():\par
321                 self.db.finaliza_Ponto(id_funcionario,data_Atual(True),2)\par
322             #   Ponto normal\par
323             elif ((ponto_antigo['hora_final']-limite_inferior_saida) <= data_Atual()) and ((ponto_antigo['hora_final']+limite_superior_saida) >= data_Atual()):\par
324                 self.db.finaliza_Ponto(id_funcionario,data_Atual(True),1)\par
325                 return\par
326 \par
327         id_horario=self.db.buscar_Horario_Mais_Proximo_de_Funcionario(id_funcionario,get_Week_Day(),limite_inferior_entrada,limite_superior_entrada)\par
328         if id_horario!=False:\par
329             self.db.criar_Ponto(id_funcionario,id_horario,-1)\par
330             return\par
\par
}
}
